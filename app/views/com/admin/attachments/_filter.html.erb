<%= form_with theme: 'search', model: ActiveStorage::Attachment.new do |f| %>
  <div class="fields">
    <%= f.text_field :record_type %>
    <%= f.text_field :record_id %>
    <%= f.text_field :name %>
    <%= f.submit %>
    <%= link_to t('.clear'), filter_params(except: [:record_type, :record_id, :name]), class: 'ui button' %>
  </div>
<% end %>


